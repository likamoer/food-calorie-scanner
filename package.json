{
  "name": "food-calorie-scanner",
  "version": "1.0.0",
  "description": "Full-stack food image recognition and calorie calculation app with React frontend and Node.js backend",
  "main": "server/server.js",
  "scripts": {
    "start": "npm run build && npm run start:server",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cd server && npm run dev",
    "dev:client": "cd client && npm start",
    "start:server": "cd server && npm start",
    "build": "cd client && npm run build",
    "build:client": "cd client && npm run build",
    "build:server": "cd server && npm install",
    "install:all": "npm install && cd server && npm install && cd ../client && npm install",
    "test": "cd client && npm test && cd ../server && npm test",
    "clean": "rm -rf client/build server/uploads/* client/node_modules server/node_modules node_modules"
  },
  "keywords": [
    "food",
    "calorie",
    "recognition",
    "h5",
    "mobile",
    "web-app",
    "image-processing",
    "nutrition",
    "health",
    "ai"
  ],
  "author": "Your Name",
  "license": "MIT",
  "homepage": "https://github.com/yourusername/food-calorie-scanner",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/food-calorie-scanner.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/food-calorie-scanner/issues"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "http-server": "^14.1.1"
  },
  "optionalDependencies": {
    "eslint": "^8.0.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "iOS >= 12",
    "Android >= 6"
  ],
  "pwa": {
    "name": "食物卡路里扫描器",
    "short_name": "卡路里扫描",
    "description": "智能识别食物照片并计算卡路里",
    "start_url": "/",
    "display": "standalone",
    "theme_color": "#4A90E2",
    "background_color": "#ffffff"
  }
}
